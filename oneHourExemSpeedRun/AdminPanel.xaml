<Window x:Class="oneHourExemSpeedRun.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oneHourExemSpeedRun"
        mc:Ignorable="d"
                Title="KompService" Height="450" Width="800" MaxHeight="600" MaxWidth="1600" MinHeight="300" MinWidth="500">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="53*"/>
            <RowDefinition Height="360*"/>
            <RowDefinition Height="21*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="654*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="1" Margin="0,10,0,0" Grid.Row="1">
            <DataGrid x:Name="dgOrderList" Height="100"/>
        </StackPanel>
        <Label Grid.Column="1" Content="Панель администратора" FontSize="14" Margin="252,0,149,0" VerticalAlignment="Center" Height="25"/>

    </Grid>
</Window>
